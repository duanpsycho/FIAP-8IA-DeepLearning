{"nbformat":4,"nbformat_minor":0,"metadata":{"colab":{"name":"8IA-DeepLearning-Aula1-Exercicio2","provenance":[]},"kernelspec":{"name":"python3","display_name":"Python 3"}},"cells":[{"cell_type":"code","metadata":{"id":"1y_G_28AiyZc","colab_type":"code","colab":{}},"source":["# -*- coding: utf-8 -*-\n","\"\"\"\n","Created on Thu May 23 15:36:53 2019\n","\n","@author: felip\n","\"\"\"\n","import sklearn.datasets\n","import numpy as np\n","import pandas as pd\n","import matplotlib.pyplot as plt\n","from sklearn.model_selection import train_test_split\n","from sklearn import metrics "],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"UUtiFcgcjB7M","colab_type":"code","colab":{}},"source":["class Perceptron:\n","  \n","  #constructor\n","  def __init__ (self):\n","    self.w = None\n","    self.th = None\n","    \n","  #model  \n","  def model(self, x):\n","    return 1 if (np.dot(self.w, x) >= self.th) else 0\n","  \n","  #predictor to predict on the data based on w\n","  def predict(self, X):\n","    Y = []\n","    for x in X:\n","      result = self.model(x)\n","      Y.append(result)\n","    return np.array(Y)\n","    \n","  def fit(self, X, Y, epochs = 1, lr = 1):\n","    self.w = np.ones(X.shape[1])\n","    self.th = 0\n","    accuracy = {}\n","    max_accuracy = 0\n","    wt_matrix = []\n","    #for all epochs\n","    for i in range(epochs):\n","      for x, y in zip(X, Y):\n","        y_pred = self.model(x)\n","        if y == 1 and y_pred == 0:\n","          self.w = self.w + lr * x\n","          self.th = self.th - lr * 1\n","        elif y == 0 and y_pred == 1:\n","          self.w = self.w - lr * x\n","          self.th = self.th + lr * 1\n","          \n","      wt_matrix.append(self.w)    \n","      accuracy[i] = metrics.accuracy_score(self.predict(X), Y)\n","      if (accuracy[i] > max_accuracy):\n","        max_accuracy = accuracy[i]\n","        chkptw = self.w\n","        chkptb = self.th\n","    \n","    print(accuracy[i])\n","    #checkpoint (Save the weights and b value)    \n","    self.w = chkptw\n","    self.th = chkptb\n","        \n","    print(max_accuracy)\n","    #plot the accuracy values over epochs\n","    plt.plot(accuracy.values())\n","    plt.xlabel(\"Epoch #\")\n","    plt.ylabel(\"Accuracy\")\n","    plt.ylim([0, 1])\n","    plt.figure(1)\n","    plt.show()\n","    \n","    #return the weight matrix, that contains weights over all epochs\n","    return np.array(wt_matrix)"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"Nn37dnfGjFT8","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":351},"outputId":"9774b754-b16a-4624-8e62-6625889161a1","executionInfo":{"status":"ok","timestamp":1573091388417,"user_tz":180,"elapsed":1091,"user":{"displayName":"Duan Nunes Alves da Silva","photoUrl":"https://lh3.googleusercontent.com/a-/AAuE7mDfouVJ_YzmdNjtxGrHIL2yKbfjANwHO8v8Y5mSyg=s64","userId":"04410361390824864667"}}},"source":["#load the breast cancer data\n","breast_cancer = sklearn.datasets.load_breast_cancer()\n","\n","#convert the data to pandas dataframe.\n","data = pd.DataFrame(breast_cancer.data, columns = breast_cancer.feature_names)\n","data[\"class\"] = breast_cancer.target\n","data.head()\n","data.describe()"],"execution_count":10,"outputs":[{"output_type":"execute_result","data":{"text/html":["<div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>mean radius</th>\n","      <th>mean texture</th>\n","      <th>mean perimeter</th>\n","      <th>mean area</th>\n","      <th>mean smoothness</th>\n","      <th>mean compactness</th>\n","      <th>mean concavity</th>\n","  