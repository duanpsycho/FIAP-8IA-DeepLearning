,"        if Pclass == 1:\n","            return 37\n","        elif Pclass == 2:\n","            return 29\n","        else:\n","            return 24\n","    else:\n","        return Age"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"PJoZ3rK0ubw5","colab_type":"code","colab":{}},"source":["titanic_data['Age'] = titanic_data[['Age', 'Pclass']].apply(age_approx, axis=1)\n","titanic_data.dropna(inplace=True)\n","gender = pd.get_dummies(titanic_data['Sex'],drop_first=True)\n","embark_location = pd.get_dummies(titanic_data['Embarked'],drop_first=True)\n","titanic_data.drop(['Sex', 'Embarked'],axis=1,inplace=True)\n","titanic_dmy = pd.concat([titanic_data,gender,embark_location],axis=1)\n","titanic_dmy.drop(['Pclass'],axis=1,inplace=True)\n","titanic_dmy.drop(['Q'],axis=1,inplace=True)\n","\n","X = titanic_dmy.iloc[:,[1,2,3,4,5,6]].values\n","y = titanic_dmy.iloc[:,0].values\n","\n","X = StandardScaler().fit_transform(X)\n","X_train, X_test, y_train, y_test = train_test_split(X, y, test_size = .3, random_state=25)\n","\n","# \n","nh = 10"],"execution_count":0,"outputs":[]},{"cell_type":"code","metadata":{"id":"cGzP-VU9u4_f","colab_type":"code","colab":{"base_uri":"https://localhost:8080/","height":34},"outputId":"307d3adf-1ef3-41dc-8cc4-85031110dc88","executionInfo":{"status":"ok","timestamp":1573329303366,"user_tz":180,"elapsed":781,"user":{"displayName":"Duan Nunes Alves da Silva","photoUrl":"https://lh3.googleusercontent.com/a-/AAuE7mDfouVJ_YzmdNjtxGrHIL2yKbfjANwHO8v8Y5mSyg=s64","userId":"04410361390824864667"}}},"source":["#Primeiro EML baseado em MPL com função de ativação sigmoid\n","srhl_sigmoid = MLPRandomLayer(n_hidden=nh, activation_func='sigmoid')\n","elm_model = GenELMClassifier(hidden_layer=srhl_sigmoid)\n","elm_model.fit(X_train, y_train)\n","score = elm_model.score(X_test, y_test)\n","print(score)"],"execution_count":10,"outputs":[{"output_type":"stream","text":["0.7752808988764045\n"],"name":"stdout"}]},{"cell_type":"code","met